{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "COHERE_API_KEY": "@cohere_api_key",
    "QDRANT_URL": "@qdrant_url",
    "QDRANT_API_KEY": "@qdrant_api_key",
    "QDRANT_CLUSTER_ID": "@qdrant_cluster_id",
    "NEON_DB_URL": "@neon_db_url"
  }
}